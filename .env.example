# Workspace
WORKSPACE_DIR=workspace

# Agent 선택
ACTIVE_AGENT=auto

# OpenRouter
OPENROUTER_API_KEY=__PUT_YOUR_KEY_HERE__
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1

# 모델 정책(권장)
OPENROUTER_MODEL_PRIMARY=anthropic/claude-3.5-sonnet
OPENROUTER_MODEL_FALLBACK=openai/gpt-4o-mini

# LLM 실행 옵션
LLM_TIMEOUT_SEC=45
LLM_MAX_RETRIES=1
LLM_MAX_TOKENS=900
LLM_TEMPERATURE=0.2
LLM_ENABLED=True

# OpenRouter 권장 헤더(옵션: 회사 정책에 맞게)
OPENROUTER_APP_TITLE=dia-agent-platform
OPENROUTER_HTTP_REFERER=http://localhost

# 회사 SSL 정책 우회 (CRL 체크 비활성화)
PYTHONHTTPSVERIFY=0
SSL_NO_VERIFY=1
